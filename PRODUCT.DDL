CREATE TABLE PRODUCT(
PRODUCT_ID				UUID PRIMARY KEY NOT NULL,
DESCRIPTION				VARCHAR,
ITEM_LOOKUP_CODE		VARCHAR,
PRICE					NUMERIC,
ITEM_TYPE				INTEGER,
COST					NUMERIC,
QUANTITY				INTEGER,
REORDER_POINT			INTEGER,
RESTOCK_LEVEL			INTEGER,
PARENT_ITEM				UUID,
EXTENDED_DESCRIPTION	VARCHAR,
ACTIVE_STATUS			BOOLEAN,
MSRP					NUMERIC,
DATE_CREATED			DATE
);